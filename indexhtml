<li id="emailLi"><b class="font-yellow">EMAIL</b> : <span id="email">pubudupraneeth@gmail.com</span></li>

<script>
	// Get references to the li and email elements
	const emailLi = document.getElementById("emailLi");
	const emailElement = document.getElementById("email");
	
	// Add a click event listener to the li element
	emailLi.addEventListener("click", function() {
		copyEmail(emailElement.innerText);
	});

	function copyEmail(emailText) {
		const tempInput = document.createElement("input");
		document.body.appendChild(tempInput);
		tempInput.value = emailText;
		tempInput.select();
		document.execCommand("copy");
		document.body.removeChild(tempInput);
		alert("Email address copied to clipboard: " + emailText);
	}
</script>

